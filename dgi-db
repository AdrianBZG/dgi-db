<VirtualHost *:80>
   ServerName vm8
   ServerAlias dgidb.genome.wustl.edu *.dgidb.genome.wustl.edu dgidb.org *.dgidb.org
   DocumentRoot /var/www/dgi-db/public
   PassengerHighPerformance on
   <Directory /var/www/dgi-db/public>
      AllowOverride all
      Options -MultiViews
   </Directory>
   AddOutputFilterByType DEFLATE text/html text/css text/plain text/xml application/json
   AddOutputFilterByType DEFLATE image/jpeg, image/png, image/gif
</VirtualHost>
