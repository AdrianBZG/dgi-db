.span6
  .well
    %p
      %h3
        =title
        %small
          %strong
            %i.icon-flag.tip{title: tooltip_text}
      %small
        %strong
          %em
            =subtitle
    %dl
      -results.each do |result|
        -result.genes.each do |gene|
          %dt=link_to gene.name, gene_path(gene.name)
          %dd="For search term: #{result.search_term}"
