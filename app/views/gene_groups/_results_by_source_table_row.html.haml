%tr
  %td
    =link_to results_by_source_table_row.name, gene_group_by_category_path(results_by_source_table_row.name)
  -results_by_source_table_row.sources.each do |in_source|
    %td
      -if in_source
        %i.icon-ok
          %p(style="display:none")a
      -else
        %i
          %p(style="display:none")z
